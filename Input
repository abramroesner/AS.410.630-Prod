import re
 
def input_fasta(fileid):

    with open(fileid, 'r') as f:
        sequencedict = {}
        sequenceid = 0
        catstring = ""  
        for line in f: 
            line = line.replace(" ", "").upper()  
            if line.startswith('>'):
                if sequenceid != 0:
                    sequencedict[sequenceid] = catstring 
                sequenceid = line[1:-1]
                catstring = ""
            else:
                catstring += line[:-1]

        sequencedict[sequenceid] = catstring

    return sequencedict

fileid = input('Please Input FASTA FileID :')
try:
    orfval = int(input("(Default 50) Please Input the Minimum ORF Value: "))
except:
    orfval = 50
    
output = input_fasta(fileid)
print(output)

#Realized the program needs to iterate through the file as a portion of input
allsequence = input_fasta(fileid) #read the file

